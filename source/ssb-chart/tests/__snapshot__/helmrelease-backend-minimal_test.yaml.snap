should render Deployment:
  1: |
    apiVersion: apps/v1
    kind: Deployment
    metadata:
      labels:
        app: unittest-app
        billing-project: ssb-unittest
        chart: ssb-chart-4.0.1
        heritage: Helm
        release: unittest-app
      name: unittest-app
    spec:
      replicas: 1
      selector:
        matchLabels:
          app: unittest-app
          release: unittest-app
      template:
        metadata:
          labels:
            app: unittest-app
            release: unittest-app
        spec:
          containers:
            - image: ssb.no/unittest-app:1.0
              imagePullPolicy: Always
              name: unittest-app-cont
              ports:
                - containerPort: 80
                  name: http-main
              resources:
                limits:
                  memory: 128Mi
                requests:
                  cpu: 100m
                  memory: 128Mi
              securityContext:
                allowPrivilegeEscalation: false
                capabilities:
                  drop:
                    - ALL
                privileged: false
                readOnlyRootFilesystem: true
                runAsGroup: 1000
                runAsNonRoot: true
                runAsUser: 1000
                seccompProfile:
                  type: RuntimeDefault
              volumeMounts:
                - mountPath: /tmp
                  name: tmp-volume
          securityContext:
            seccompProfile:
              type: RuntimeDefault
          serviceAccountName: unittest-app-sa
          volumes:
            - emptyDir: {}
              name: tmp-volume
